	operator with several different types
	   Written by:
	   Date:
*/
#include <iostream>
using namespace std;

int main ()
{
//	Statements 
	cout << 24      << "\n";        // Print an integer
	cout << 12.3    << "\n";        // Print a float
	cout << 'A'     << "\n";        // Print a character
	cout << "Hello" << "\n";        // Print a string
	return 0;
}	// main 

/*	Results:

	24 
	12.3 
	A 
	Hello

*/
